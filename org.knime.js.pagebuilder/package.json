{
  "name": "knime-pagebuilder",
  "version": "1.0.0",
  "description": "PageBuilder for KNIMEÂ® Analytics Platform & WebPortal",
  "private": true,
  "scripts": {
    "dev": "npm run opensourcecredits -- -- -- --no-overwrite && vue-cli-service serve src/dev.js",
    "dev:integration": "concurrently -n server,app -c yellow,blue \"http-server -c-1 --cors -p 4000 dist/ http-server\" \"npm run build:credits && npm run build:lib -- --watch\"",
    "build": "npm run build:credits && npm run build:lib && npm run build:wrapper",
    "build:credits": "npm run opensourcecredits && vue-cli-service build --target lib --formats umd-min --name knime-pagebuilder-credits src/components/PageBuilderCredits.vue",
    "build:lib": "vue-cli-service build --no-clean --target lib --formats umd-min src/components/PageBuilder.vue",
    "build:lib:dev": "npm run build:lib -- --watch",
    "build:wrapper": "cross-env NODE_ENV=production vue-cli-service build --no-clean --inline-vue src/main.js",
    "inspect": "vue-cli-service inspect",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint --ext .js,.vue .",
    "lint:css": "stylelint '**/*.{css,vue}'",
    "audit": "check-audit --production",
    "test:unit": "vue-cli-service test:unit",
    "coverage": "vue-cli-service test:unit --coverage",
    "coverage:watch": "npm run test:unit -- --coverage --watch",
    "sendcoverage": "cross-var sonar-scanner -Dsonar.login=\"$SONAR_LOGIN\" -Dsonar.password=\"$SONAR_PASSWORD\"",
    "opensourcecredits": "npm run --prefix webapps-common/ opensourcecredits"
  },
  "license": "UNLICENSED",
  "author": "KNIME AG, Zurich, Switzerland",
  "dependencies": {
    "@popperjs/core": "2.5.3",
    "consola": "2.10.1",
    "core-js": "3.6.5",
    "date-fns": "2.16.1",
    "date-fns-tz": "1.0.12",
    "filesize": "6.1.0",
    "iframe-resizer": "4.2.10",
    "v-calendar": "2.0.1",
    "vue": "2.6.11",
    "vue-clickaway": "2.2.2",
    "vue-slider-component": "3.1.5",
    "vuex": "3.0.1",
    "webapps-common": "file:webapps-common"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "4.5.4",
    "@vue/cli-plugin-unit-jest": "4.5.4",
    "@vue/cli-service": "4.5.4",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "concurrently": "5.3.0",
    "cross-env": "5.2.0",
    "cross-var": "1.1.0",
    "eslint": "5.16.0",
    "eslint-plugin-vue": "7.10.0",
    "eslint-plugin-jest-formatting": "3.0.0",
    "fontsource-roboto": "3.0.3",
    "fontsource-roboto-mono": "3.0.3",
    "http-server": "0.12.3",
    "jest-junit": "7.0.0",
    "modern-normalize": "1.1.0",
    "npm-audit-resolver": "2.3.1",
    "postcss-import": "12.0.1",
    "postcss-preset-env": "6.7.0",
    "postcss-url": "8.0.0",
    "raw-loader": "3.1.0",
    "sonar-scanner": "3.1.0",
    "stylelint": "13.13.1",
    "stylelint-config-standard": "20.0.0",
    "svgo-loader": "2.2.1",
    "vue-svg-loader": "0.16.0",
    "vue-template-compiler": "2.6.11"
  },
  "engines": {
    "node": "14.x"
  }
}
